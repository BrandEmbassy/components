{
  "name": "components",
  "version": "0.1.0",
  "main": "lib/index.js",
  "dependencies": {
    "autoprefixer": "7.1.6",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "object-assign": "4.1.1",
    "openurl": "^1.1.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-daterange-picker": "^1.2.1",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-highlight": "^0.10.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^7.0.0",
    "style-loader": "^0.19.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.2",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-flow-react-proptypes": "^22.0.0",
    "babel-plugin-react-css-modules": "^3.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.26.0"
  },
  "scripts": {
    "jest": "jest",
    "start": "node scripts/start.js",
    "postinstall": "yarn run build",
    "build": "yarn run lib",
    "pretest": "yarn run lint && npm run flow",
    "test": "yarn run jest",
    "lint": "standard './src/**/*.js' './stories/**/*.js' './src/**/*.jsx'",
    "lib": "babel src/package --out-dir lib --copy-files",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "library": "yarn run build && yarn run build-storybook && node scripts/storybook.js && yarn run storybook",
    "concat-css": "cat src/styles/Grid.css build/static/css/main.css > build/static/css/components.css && yarn run cleanup",
    "cleanup": "rm -f build/static/css/main.css && rm -r build/static/js",
    "flow": "flow check",
    "flow:watch": "flow",
    "flow-typed": "flow-typed install",
    "build:ostrava": "node scripts/build.js && concact-css"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/package/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/src/testSetup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0",
      "flow"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [],
    "globals": [
      "fetch",
      "URL",
      "shallow",
      "render",
      "mount"
    ],
    "env": [
      "jest",
      "browser",
      "es6"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.9",
    "@storybook/addon-knobs": "^3.4.2",
    "@storybook/addon-links": "^3.3.9",
    "@storybook/addons": "^3.3.9",
    "@storybook/react": "^3.3.9",

    "dotenv": "4.0.0",
    "dotenv-expand": "4.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "flow-bin": "^0.71.0",
    "flow-typed": "^2.4.0",
    "jest": "20",
    "js-beautify": "^1.7.5",
    "postcss": "^6.0.14",
    "postcss-cssnext": "^3.0.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-import": "^11.0.0",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^2.0.10",
    "react-storybook-addon-chapters": "^2.1.3",
    "standard": "^11.0.1"
  },
  "postcss": {
    "parser": "sugarss",
    "map": false,
    "from": "/path/to/src.sss",
    "to": "/path/to/dest.css",
    "plugins": {
      "postcss-plugin": {}
    }
  }
}
